	volume = {19},
	issn = {0959-4965},
	shorttitle = {Emotional responses to music},
	url = {http://ezp1.harvard.edu/login?url=http://ovidsp.ovid.com/ovidweb.cgi?T=JS&CSC=Y&NEWS=N&PAGE=fulltext&D=ovftj&AN=00001756-200801080-00014},
