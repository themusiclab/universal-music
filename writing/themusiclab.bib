@article{Gingras2014,
	title = {Exploring {Shamanic} {Journeying}: {Repetitive} {Drumming} with {Shamanic} {Instructions} {Induces} {Specific} {Subjective} {Experiences} but {No} {Larger} {Cortisol} {Decrease} than {Instrumental} {Meditation} {Music}},
	volume = {9},
