	year = {2020},
	pmid = {31871195},
	note = {tex.ids: Hattori2019},
	keywords = {music, to read, evolution, dance, chimpanzees, rhythmic movement},
	pages = {936--942},
	file = {Full Text PDF:/Users/Lidya/Zotero/storage/ESUT8XJK/Hattori and Tomonaga - 2019 - Rhythmic swaying induced by sound in chimpanzees (.pdf:application/pdf;Hattori and Tomonaga - 2020 - Rhythmic swaying induced by sound in chimpanzees (.pdf:/Users/Lidya/Zotero/storage/7SUU5N9Y/Hattori and Tomonaga - 2020 - Rhythmic swaying induced by sound in chimpanzees (.pdf:application/pdf;Snapshot:/Users/Lidya/Zotero/storage/NITXDNIX/1910318116.html:text/html},
