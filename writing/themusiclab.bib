	author = {Freitas, Julian De and Thomas, Kyle and DeScioli, Peter and Pinker, Steven},
	month = jul,
	year = {2019},
	pmid = {31253709},
	keywords = {theory of mind, cooperation, bystander effect, common knowledge, coordination},
	pages = {13751--13758},
}

