@article{Libben2008,
	title = {The multidetermined nature of idiom processing},
	volume = {36},
	issn = {1532-5946},
	url = {https://doi.org/10.3758/MC.36.6.1103},
	doi = {10.3758/MC.36.6.1103},
	abstract = {Models of idiom comprehension differ in their predictions concerning compositionality: Some claim that idiomatic meaning is the result of compositional analysis initiated at the earliest stages of comprehension, whereas others claim that compositional analysis occurs only at late stages, subsequent to direct retrieval—especially for idioms that are highly familiar. We evaluated these alternatives in four experiments by using a variety of online and offline comprehension measures. In Experiment 1, we analyzed the normative characteristics of 219 idioms with respect to these predictions. Dimensions of interest included several measures of decomposability, familiarity, and word frequency of the idioms’ verbs and nouns. In Experiments 2 through 4, we determined how these dimensions relate to several online measures of idiom comprehension. High familiarity was associated with good comprehension across all experiments; however, facilitative effects of decomposability were found only for tasks that required an overt semantic judgment. Word frequency, but not semantic decomposability of the idiom-initial verb, was associated with comprehension for some measures. These data support a model of idiom comprehension, according to which figurative meaning arises from the time-dependent availability of multiple linguistic constraints, and in which decomposability plays a limited role in the earliest stages of idiom comprehension. Normative data for 210 of the idiomatic phrases may be downloaded from the Psychonomic Society Web archive at www.psychonomic.org/archive/.},
	language = {en},
	number = {6},
	urldate = {2019-01-23},
	journal = {Memory \& Cognition},
	author = {Libben, Maya R. and Titone, Debra A.},
	month = sep,
	year = {2008},
	keywords = {Figurative Meaning, Idiomatic Meaning, Idiomatic Phrase, Literal Sentence, Noun Relatedness},
	pages = {1103--1121},
}

