	author = {Cubuk, Ekin D. and Zoph, Barret and Mane, Dandelion and Vasudevan, Vijay and Le, Quoc V.},
	month = apr,
	year = {2019},
	note = {arXiv: 1805.09501},
	keywords = {Statistics - Machine Learning, ToDo, Computer Science - Computer Vision and Pattern Recognition, Computer Science - Machine Learning},
