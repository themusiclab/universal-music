	annote = {awesome. erp study, 2-3 DAY OLD neonates show more activation when the downbeat is omitted from a drum pattern},
	file = {Winkler et al. - 2009 - Newborn Infants Detect the Beat in Music.pdf:/Users/Lidya/Zotero/storage/HN53CUCR/Winkler et al. - 2009 - Newborn Infants Detect the Beat in Music.pdf:application/pdf},
