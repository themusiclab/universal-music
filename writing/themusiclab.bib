	author = {Loewy, Joanne},
	month = mar,
	year = {2015},
	keywords = {added, neonatal lullabies, NICU music therapy, song of kin},
	pages = {178--185},
}

@inproceedings{Loughran2015,
	title = {Tonality driven piano compositions with grammatical evolution},
