	language = {en},
	number = {10},
	urldate = {2019-08-18},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Levitin, Daniel J. and Chordia, Parag and Menon, Vinod},
	month = mar,
	year = {2012},
	pmid = {22355125},
	keywords = {1/f distributions, fractal mathematics, musical structure, temporal perception},
