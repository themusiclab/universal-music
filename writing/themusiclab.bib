@article{Salimpoor2011,
	title = {Anatomically distinct dopamine release during anticipation and experience of peak emotion to music},
	volume = {14},
	copyright = {© 2011 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
	issn = {1097-6256},
	url = {http://www.nature.com/neuro/journal/v14/n2/full/nn.2726.html},
	doi = {10.1038/nn.2726},
	abstract = {Music, an abstract stimulus, can arouse feelings of euphoria and craving, similar to tangible rewards that involve the striatal dopaminergic system. Using the neurochemical specificity of [11C]raclopride positron emission tomography scanning, combined with psychophysiological measures of autonomic nervous system activity, we found endogenous dopamine release in the striatum at peak emotional arousal during music listening. To examine the time course of dopamine release, we used functional magnetic resonance imaging with the same stimuli and listeners, and found a functional dissociation: the caudate was more involved during the anticipation and the nucleus accumbens was more involved during the experience of peak emotional responses to music. These results indicate that intense pleasure in response to music can lead to dopamine release in the striatal system. Notably, the anticipation of an abstract reward can result in dopamine release in an anatomical pathway distinct from that associated with the peak pleasure itself. Our results help to explain why music is of such high value across all human societies.},
	language = {en},
	number = {2},
	urldate = {2014-05-21},
	journal = {Nature Neuroscience},
	author = {Salimpoor, Valorie N. and Benovoy, Mitchel and Larcher, Kevin and Dagher, Alain and Zatorre, Robert J.},
	month = feb,
	year = {2011},
	keywords = {neuro},
	pages = {257--262},
}

