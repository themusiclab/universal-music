	file = {Full Text PDF:/Users/Lidya/Zotero/storage/2CUBK6U3/Marlowe - 2005 - Hunter-gatherers and human evolution.pdf:application/pdf;Snapshot:/Users/Lidya/Zotero/storage/97H5VKP3/evan.html:text/html},
